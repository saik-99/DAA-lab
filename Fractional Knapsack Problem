# include<stdio.h>
void knapsack(int n, float weight[], float profit[], float capacity) {
  // write your code here
	float totalprofit = 0.0; 
	int remainingspace = capacity;
	for (int i=0; i < n && remainingspace > 0; i++){
		if (weight[i] <= remainingspace){
			remainingspace -= weight[i];
			totalprofit += profit[i];
		} else {
			float fractionalpart = (float)remainingspace / weight[i];
			totalprofit += profit[i] * fractionalpart;
			remainingspace = 0;
		}
	}
	printf("Maximum profit is:- %f\n",totalprofit);
}

int main() {
	float weight[20], profit[20], capacity;
	int num, i, j;
	float ratio[20], temp;
	printf("Enter the no. of objects: ");
	scanf("%d", &num);
	printf("Enter the weights and profits of each object:\n");
	for (i = 0; i < num; i++) {
		scanf("%f %f", &weight[i], &profit[i]);
	}
	printf("Enter the capacity of knapsack:");
	scanf("%f", &capacity);
	for (i = 0; i < num; i++) {
		ratio[i] = profit[i] / weight[i];
	}

	for (i = 0; i < num; i++) {
		for (j = i + 1; j < num; j++) {
}
